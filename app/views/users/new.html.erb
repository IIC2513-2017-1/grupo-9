<h1> Create user </h1>

<% if flash[:notice] %>
<%= flash[:notice] %>
<% end %>

<%= form_for @user do |f| %>

<%= f.label :first_name %>
<%= f.text_field :first_name %>
<br>
<%= f.label :last_name %>
<%= f.text_field :last_name %>
<br>
<%= f.label :email %>
<%= f.text_field :email %>
<br>
<%= f.label :password%>
<%= f.text_field :password %>
<br>
<%= f.submit "Create user" %>
<% end %>
